nav.nav
    ul.nav__mainMenu
        .container
            .nav__mainMenuItemsContainer
                each mainMenu in navContent
                    li.nav__mainMenuItem
                        a(href=`${mainMenu.mainMenuPath}`)=mainMenu.mainMenuTitle
                        if mainMenu.subMenu || mainMenu.subMenuProducts
                            ul.nav__subMenu
                                each subMenu in mainMenu.subMenu
                                    li.nav__subMenuList
                                        h3
                                            a(href=`${subMenu.subMenuPath}`)=subMenu.subMenuTitle
                                        .nav__subMenuListsContainer
                                            if subMenu.subMenuList
                                                ul
                                                    each list in subMenu.subMenuList
                                                        li
                                                            a(href=`${list.linkPath}`)=list.title
                                            if subMenu.subMenuList2
                                                ul
                                                    each list in subMenu.subMenuList2
                                                        li
                                                            a(href=`${list.linkPath}`)=list.title
                                if mainMenu.subMenuProducts
                                    ul.nav__prductCards
                                        each product in mainMenu.subMenuProducts
                                            li
                                                a(href=`${product.linkPath}`)
                                                    img(src=`${product.imgPath}`, alt=`${product.title}`)
                                                    h4=product.title
                                                    p=product.subTitle
                                                    span=product.cost